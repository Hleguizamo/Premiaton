        <script type="text/javascript">
            setLi();
            function setLi(){
                {% if app.user and is_granted('ROLE_ADMINISTRATOR') %}
                {% if app.user.superadmin == 1 %} 
                  var basemenu = 748;
                {% else %} 
                  var basemenu = 380;
                {% endif %}
                {% elseif app.user and is_granted('ROLE_USER') %}
                  var basemenu = 380;
                {% endif %} 
                var newmargin = ($( window ).width() - basemenu) / 2;
                if($( window ).width() <= basemenu){
                    $( "#menuLi" ).css('margin-left', '0px');
                }else{
                    $( "#menuLi" ).css('margin-left', newmargin + 'px');  
                }
            }
            $( window ).resize(function() {
                setLi();
            });
        </script>
